<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Age__c</fullName>
    <description>This field calculates the contact&apos;s age based on their birthdate</description>
    <externalId>false</externalId>
    <formula>FLOOR(( TODAY()  -  Birthdate ) / 365)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Contact&apos;s age based on their birthdate</inlineHelpText>
    <label>Age</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
